function createlink(subject,path){
	var subjectlink = createNewElement("a",subject)
	var dataremotelink = appendAttribute(subjectlink,"data-remote","true")
		subjectlink.href = path 

	return subjectlink

}
function createSongParagraf(id,song){
	var p = createNewElement("p",song)

	var attribute = appendAttribute(p,"ondblclick","updatePlayListAndPlay(" + id + ")")
	return p
}
function appendAlbumInfo(artist,album,albumart){
	var albumInfoDiv = document.getElementById("album-info")
	var albumart = createImageTag(albumart,album)
		albumart.height = 200
		var artistparagraf = createNewElement("p","Artist:")
		artistparagraf.appendChild(createlink(artist["name"],"artists/"+artist["id"]+"/"))
	 
		var albumparagraf = createNewElement("p","Album:" + album)
		albumInfoDiv.appendChild(albumart)
		albumInfoDiv.appendChild(artistparagraf)
		albumInfoDiv.appendChild(albumparagraf)
	return albumInfoDiv
}
function appendTo(type,data){
	var element  = createNewElement(type)
		element.appendChild(data)
	return element
}
function createNewElement(element,value){
	var newElement = document.createElement(element)
	
	if(value === undefined){
		newElement.innerHTML = ""
	}else{
		newElement.innerHTML = value
	}
	return newElement
	
}
function createTableHeaders(table,header){
	
		
	
	for (var i = 0; i < header.length;i++) {
		var h = createNewElement("th")
		h.innerHTML += header[i];
	
		table.appendChild(h)
	}

	return table
}


function appendAttribute(subject,attribute,value){
	var newAttribute = document.createAttribute(attribute)
	newAttribute.value = value

	return subject.setAttributeNode(newAttribute)

}
function createImageTag(source,text,a){

	var img = createNewElement("img")
	img.src = source
	img.alt = text
	if(a === undefined){
		return img
	}
	a.title = text
	a.appendChild(img)
	
	return a
}
